{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {},
  "items": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "origin-php-sample"
      },
      "spec": {},
      "status": {
        "dockerImageRepository": ""
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "source-build"
      },
      "spec": {
        "triggers": [],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/openshift-qe/incremental-app.git"
          }
        },
        "strategy": {
          "type": "Source",
          "sourceStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "ruby:2.0",
              "namespace": "openshift"
            },
            "incremental": true
          }
        }
      }
    }
  ]
}
