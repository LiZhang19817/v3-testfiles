{
    "apiVersion": "v1",
    "kind": "List",
    "items": [
        {
      "kind": "Pod",
      "apiVersion":"v1",
    
      "metadata": {
            "name": "hello-pod",
            "labels": {
                    "name": "hello-pod"
            }
      },
    
      "spec": {
          "containers": [{
            "name": "hello-pod",
            "image": "aosqe/hello-openshift",
            "ports": [
              {
                "containerPort": 8081
              }
            ]
          }]
      }
    },
    {
        "kind": "Service",
        "apiVersion": "v1",
        "metadata": {
            "name": "hello-pod",
            "labels":{"name":"hello-pod"}
        },
        "spec": {
            "ports": [
                    {
                            "name": "http",
                            "protocol": "TCP",
                            "port": 27017,
                             "targetPort": 8081
                           
                    }
            ],
            "selector": {
                    "name": "hello-pod"
            }
        }
    }
  ]
}
